{
  "ignoreFiles": ["**/*.js","**/*.ts","**/*.tsx", "node_modules", "build"],
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-rational-order"
  ],
  "rules": {
    "indentation": 4,
    "no-missing-end-of-source-newline": false,
    "no-descending-specificity": null,
    "function-calc-no-invalid": null
  }
}